{
  "manifest_version": 2,
  "name": "MyBookmarks",
  "author": "Balavignesh",
  "version": "1.0.1",
  "description": "Replace new tab screen with your bookmarked links.",
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["bookmarks" , "history"] 
}